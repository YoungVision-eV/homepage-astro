{
  "problemMatcher": [
    {
      "owner": "svelte-check",
      "pattern": [
        {
          "regexp": "^\\d+ (ERROR|WARNING) \"(.+)\" (\\d+):(\\d+) \"(.+)\"$",
          "severity": 1,
          "file": 2,
          "line": 3,
          "column": 4,
          "message": 5
        }
      ]
    }
  ]
}
