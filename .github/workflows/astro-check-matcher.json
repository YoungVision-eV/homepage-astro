{
  "problemMatcher": [
    {
      "owner": "astro-check",
      "pattern": [
        {
          "regexp": "^\\. lint:astro: (.+):(\\d+):(\\d+) - (error|warning) (\\w+\\(\\d+\\)): (.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "code": 5,
          "message": 6
        }
      ]
    }
  ]
}
