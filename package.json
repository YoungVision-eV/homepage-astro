    "@melt-ui/svelte": "0.83.0",
    "astro": "4.15.5",
    "clsx": "2.1.1",
    "qs": "^6.12.1",
    "svelte": "4.2.19",
    "tailwindcss": "3.4.11",
    "typescript": "5.6.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.10.0",
    "@melt-ui/pp": "0.3.2",
    "@playwright/test": "1.47.0",
    "@rgossiaux/svelte-headlessui": "2.0.0",
