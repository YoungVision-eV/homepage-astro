{
  "name": "homepage-astro",
  "type": "module",
  "private": true,
  "version": "0.0.1",
  "packageManager": "pnpm@9.12.3",
  "scripts": {
    "dev": "CMS_URL='http://localhost:3000' astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "pnpm run /^lint:.*/",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint .",
    "lint:astro-check": "astro check",
    "lint:svelte-check": "svelte-check --output machine",
    "format": "prettier --write .",
    "test": "playwright test",
    "test:ui": "pnpm run test --ui-host localhost --ui-port 33333",
    "prepare": "node .husky/install.mjs"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/svelte": "5.7.3",
    "@astrojs/tailwind": "5.1.2",
    "@melt-ui/svelte": "0.86.0",
    "astro": "4.16.10",
    "clsx": "2.1.1",
    "qs": "^6.12.1",
    "svelte": "4.2.19",
    "tailwindcss": "3.4.14",
    "typescript": "5.6.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.14.0",
    "@melt-ui/pp": "0.3.2",
    "@playwright/test": "1.48.2",
    "@rgossiaux/svelte-headlessui": "2.0.0",
    "@tailwindcss/typography": "^0.5.13",
    "@types/node": "20.17.6",
    "@types/qs": "^6.9.15",
    "@typescript-eslint/eslint-plugin": "8.14.0",
    "@typescript-eslint/parser": "8.14.0",
    "embla-carousel": "8.3.1",
    "embla-carousel-svelte": "8.3.1",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-svelte": "2.46.0",
    "git-format-staged": "3.1.1",
    "globals": "15.12.0",
    "husky": "9.1.6",
    "prettier": "3.3.3",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-astro-organize-imports": "0.4.11",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-svelte": "3.2.8",
    "prettier-plugin-tailwindcss": "0.6.8",
    "sharp": "0.33.5",
    "svelte-check": "4.0.7",
    "svelte-markdown": "0.4.1",
    "svelte-sequential-preprocessor": "2.0.2"
